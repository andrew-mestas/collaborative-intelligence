		<div class="row"><%for(var i in users) { %>

        <div class="col s6 m4">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title"><%=users[i]%></span>
             
            </div>
            <div class="card-action">
            <%if(currentUser != users[i]){%>
              <a href="./add/<%=email[i]%>">Friend Me</a>
              <!-- <a href="#">Send a message</a> -->
              <%} else {%>
              <a href="#">This is you!</a>
              <%}%>

            </div>
          </div>
        </div>

      <%}%>
      </div>
